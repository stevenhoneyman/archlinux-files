#
# build from git, but get notified of release packages
#

replace#global#source=(.*)#source=('geany::git\+https://github.com/geany/geany.git')
replace#global#sha256sums=('.*')#sha256sums=('SKIP')
replace#build#configure#autogen.sh
replace#prepare#$pkgver#master
replace#build#$pkgver#master
replace#package#$pkgver#master
